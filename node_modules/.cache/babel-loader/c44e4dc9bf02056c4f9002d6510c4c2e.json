{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HeaderOne from \"@/components/HeaderOne.vue\";\nimport { service } from '@/services/index';\nimport moment from 'moment';\nexport default {\n  components: {\n    HeaderOne\n  },\n\n  data() {\n    return {\n      tickets: []\n    };\n  },\n\n  async mounted() {\n    var data = {\n      access_token: localStorage.getItem('token')\n    };\n    var responce = await service.gettickets(data);\n\n    if (responce.ok) {\n      this.tickets = await responce.json();\n\n      for (var t in this.tickets) {\n        this.tickets[t].date_of_submission = this.date(this.tickets[t].date_of_submission);\n        console.log(this.tickets[t]);\n      }\n    }\n  },\n\n  methods: {\n    async task() {\n      this.$router.push({\n        name: 'info'\n      });\n    },\n\n    date(olddate) {\n      return moment(String(olddate)).format('DD.MM.YYYY hh:mm');\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA8CA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,SAAQC,OAAR,QAAsB,kBAAtB;AACA,OAAOC,MAAP,MAAmB,QAAnB;AAEA,eAAc;EACVC,UAAU,EAAC;IACPH;EADO,CADD;;EAIVI,IAAI,GAAE;IACF,OAAM;MACFC,OAAO,EAAE;IADP,CAAN;EAGH,CARS;;EASV,MAAMC,OAAN,GAAe;IACb,IAAIF,IAAG,GAAI;MAACG,YAAY,EAAEC,YAAY,CAACC,OAAb,CAAqB,OAArB;IAAf,CAAX;IACA,IAAIC,QAAO,GAAI,MAAMT,OAAO,CAACU,UAAR,CAAmBP,IAAnB,CAArB;;IAEA,IAAGM,QAAQ,CAACE,EAAZ,EAAe;MACb,KAAKP,OAAL,GAAe,MAAMK,QAAQ,CAACG,IAAT,EAArB;;MACA,KAAI,IAAIC,CAAR,IAAa,KAAKT,OAAlB,EAA0B;QACtB,KAAKA,OAAL,CAAaS,CAAb,EAAgBC,kBAAhB,GAAqC,KAAKC,IAAL,CAAU,KAAKX,OAAL,CAAaS,CAAb,EAAgBC,kBAA1B,CAArC;QACAE,OAAO,CAACC,GAAR,CAAY,KAAKb,OAAL,CAAaS,CAAb,CAAZ;MACJ;IACF;EACD,CApBS;;EAqBVK,OAAO,EAAC;IACJ,MAAMC,IAAN,GAAY;MACR,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAACC,IAAI,EAAE;MAAP,CAAlB;IACH,CAHG;;IAIJP,IAAI,CAACQ,OAAD,EAAS;MACT,OAAOtB,MAAM,CAACuB,MAAM,CAACD,OAAD,CAAP,CAAN,CAAwBE,MAAxB,CAA+B,kBAA/B,CAAP;IACJ;;EANI;AArBE,CAAd","names":["HeaderOne","service","moment","components","data","tickets","mounted","access_token","localStorage","getItem","respon